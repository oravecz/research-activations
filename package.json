{
  "name": "brand-activation-research",
  "version": "1.0.0",
  "type": "module",
  "description": "Brand Activation Research and Presentation System - Research, validate, and present 50 retail brand activation events with automated asset generation",
  "main": "src/index.jsx",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "validate-url": "node scripts/validate-url.js",
    "capture-screenshot": "node scripts/capture-screenshot.js",
    "create-collage": "node scripts/create-collage.js",
    "test-validation": "node scripts/validate-url.js https://example.com",
    "test-screenshot": "node scripts/capture-screenshot.js --test",
    "events:validate": "node scripts/quick-url-check.js events/details.json",
    "events:setup": "node scripts/collect-images-playwright.js setup",
    "events:logos": "node scripts/download-remaining-logos.js",
    "events:status": "node scripts/check-logo-status.js",
    "events:inventory": "node scripts/collect-images-playwright.js update-inventory",
    "events:instructions": "node scripts/collect-images-playwright.js instructions",
    "events:complete": "npm run events:setup && npm run events:logos && npm run events:inventory",
    "batch:validate": "node scripts/validate-batch.js",
    "batch:add": "node scripts/add-new-events.js",
    "cleanup:failed": "node scripts/remove-failed-events.js",
    "workflow:validate": "npm run events:validate",
    "workflow:setup": "npm run events:setup && npm run events:logos",
    "workflow:complete": "npm run events:validate && npm run events:complete && npm run build",
    "deploy:prep": "npm run workflow:complete && npm run preview",
    "deploy": "npm run build && git add -A && git commit -m 'Deploy: Update brand activation database' && git push origin main"
  },
  "keywords": [
    "react",
    "presentation",
    "brand-activation",
    "retail",
    "research",
    "screenshots",
    "image-processing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "imagesloaded": "^5.0.0",
    "lucide-react": "^0.460.0",
    "masonry-layout": "^4.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-photo-album": "^3.2.1",
    "sharp": "^0.33.5",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
